{
  "name": "PhotoPrinter",
  "version": "1.0.0",
  "main": "src/views/main.html",
  "window": {
    "title": "PhotoPrinter",
    "kiosk": true,
    "width": 800,
    "height": 480
  },
  "author": "Tobias Gall <tobias.gall@mailbox.org>",
  "license": "MIT",
  "scripts": {
    "postinstall": "cd node_modules/printer && nw-gyp rebuild --target=0.20.3",
