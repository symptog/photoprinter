  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-import": "^2.2.0",
    "nw-builder": "^3.2.0",
    "nw-gyp": "^3.4.0",
    "nwjs": "^1.4.3"
  },
  "dependencies": {
    "chokidar": "^1.6.1",
    "milligram": "^1.3.0",
    "mime": "^1.3.4",
    "navigo": "^4.7.0",
    "printer": "^0.2.2"
  }
}
